{
  "name": "vue-imagefill",
  "version": "0.1.0",
  "description": "The Vue directive for making images fill their containers",
  "main": "./src/index.js",
  "scripts": {
    "lint": "eslint --ext .js lib --ignore-pattern dist",
    "dist": "npm run lint&&webpack",
    "precommit": "npm run dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:ddvjs/vue-imagefill.git"
  },
  "keywords": [
    "vue-imagefill",
    "imagefill",
    "vue-directive",
    "vue"
  ],
  "author": "Hadi Chan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ddvjs/vue-imagefill/issues"
  },
  "jshintConfig": {
    "esversion": 6,
    "undef": true,
    "unused": true,
    "node": true,
    "predef": [
      "module",
      "require",
      "console",
      "process"
    ]
  },
  "homepage": "https://github.com/ddvjs/vue-imagefill#readme",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-component": "^1.1.1",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^3.1.0",
    "vue": "^2.5.16",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-node-externals": "^1.5.4"
  }
